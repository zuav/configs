defwinliststyle column
defwingravity   center
set padding 0 0 0 0
set border 0
set font Mono-19
set winfmt %x - %f - %n - %a
escape C-z
alias mirkwood      exec virtualbox --startvm mirkwood
alias tangorodrim   exec virtualbox --startvm tangorodrim
alias lorien        exec virtualbox --startvm lorien
alias google-chrome exec google-chrome
alias skype         exec skype
alias steam         exec steam
aleas emacs         exec emacs
aleas kpat          exec kpat
