defwinliststyle column
defwingravity   center
set padding 0 0 0 0
set border 0
set font Mono-16
set winfmt %x - %f - %n - %a
escape C-z
definekey root Tab nextscreen
definekey root f   focus
nextscreen
set font Mono-16
prevscreen
alias emacs         exec emacs -fs
alias google-chrome exec google-chrome
alias xterm         exec xterm
alias calibre       exec calibre
alias skype         exec skypeforlinux 
alias remmina       exec remmina
alias unite         exec /opt/unite/desktop/intermedia-unite
alias libreoffice   exec libreoffice
