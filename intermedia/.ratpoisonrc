defwinliststyle column
defwingravity   center
set padding 0 0 0 0
set border 0
set font Mono-19
set winfmt %x - %f - %n - %a
escape C-z
definekey root Tab nextscreen
definekey root f   focus
nextscreen
set font Mono-18
prevscreen
alias emacs         exec emacs -fs
alias google-chrome exec google-chrome
